<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/bg_round_white"
    android:orientation="vertical">


    <TextView
        android:id="@+id/title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:padding="12dp"
        android:text="提示"
        android:textSize="30sp"
        android:textColor="@color/black"
        android:background="#82948e"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/black"/>


    <RelativeLayout
        android:id="@+id/relativeLayout20"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_below="@+id/linearlayout3"
        android:background="@color/background_user"
        android:focusable="true"
        android:layout_marginTop="10dp"
        android:focusableInTouchMode="true"
        tools:ignore="UnknownIdInLayout">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/shape_wihte_frame"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:gravity="center"
                android:lines="1"
                android:padding="1dp"
                android:text="产品选择"
                android:textSize="@dimen/textSize"
                android:textStyle="bold"/>

            <View
                android:layout_width="1dip"
                android:layout_height="fill_parent"
                android:layout_centerVertical="true"
                android:layout_gravity="center_horizontal"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:layout_toRightOf="@+id/textView9_pinguan_yuanliao2"
                android:background="#EEEFFF" />

            <Spinner
                android:id="@+id/spinner2"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:spinnerMode="dropdown" />
        </LinearLayout>
    </RelativeLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp">
        <RelativeLayout
            android:id="@+id/relativeLayout1_peisongxiangqing"
            android:layout_width="240dp"
            android:layout_height="25dp" >
            <TextView
                android:id="@+id/textView1_wuliaopeisong"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:text="编号："
                android:textColor="@color/black"
                android:textSize="15sp" />
            <View
                android:id="@+id/view5"
                android:layout_width="1dip"
                android:layout_height="fill_parent"
                android:layout_centerVertical="true"
                android:layout_gravity="center_horizontal"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:layout_toEndOf="@+id/textView1_wuliaopeisong" />
            <TextView
                android:id="@+id/bianhao"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_toEndOf="@+id/view5"
                android:layout_marginLeft="4dp"
                android:layout_toRightOf="@+id/textView1_wuliaopeisong"
                android:text=""
                android:textColor="@color/black"
                android:textSize="15sp"/>
            <TextView
                android:id="@+id/zhiliangbiaozhun"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_toEndOf="@+id/view5"
                android:layout_marginLeft="4dp"
                android:layout_toRightOf="@+id/textView1_wuliaopeisong"
                android:text=""
                android:textColor="@color/black"
                android:textSize="15sp" />
        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp">


        <RelativeLayout
            android:id="@+id/relativeLayout2_peisongxiangqing"
            android:layout_width="300dp"
            android:layout_height="25dp"
            android:layout_centerHorizontal="true" >

            <TextView
                android:id="@+id/textView2_wuliaopeisong"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="名称："
                android:textColor="@color/black"
                android:textSize="15sp" />
            <View
                android:id="@+id/view3"
                android:layout_width="1dip"
                android:layout_height="fill_parent"
                android:layout_centerVertical="true"
                android:layout_gravity="center_horizontal"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:layout_toEndOf="@+id/textView2_wuliaopeisong"/>
            <TextView
                android:id="@+id/mingcheng"
                android:layout_toEndOf="@+id/view3"
                android:layout_marginLeft="4dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_toRightOf="@+id/textView2_wuliaopeisong"
                android:text=""
                android:textColor="@color/black"
                android:textSize="15sp" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp">
        <RelativeLayout
            android:id="@+id/relativeLayout3_peisongxiangqing"
            android:layout_width="170dp"
            android:layout_height="25dp"

            >

            <TextView
                android:id="@+id/textView3_wuliaopeisong"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="型号："
                android:textColor="@color/black"
                android:textSize="15sp" />
            <View
                android:id="@+id/view4"
                android:layout_toEndOf="@+id/textView3_wuliaopeisong"
                android:layout_marginLeft="4dp"
                android:layout_width="1dip"
                android:layout_height="fill_parent"
                android:layout_centerVertical="true"
                android:layout_gravity="center_horizontal"
                android:layout_marginRight="2dp"/>
            <TextView
                android:id="@+id/xinghao"
                android:layout_toEndOf="@+id/view4"
                android:layout_marginLeft="4dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_toRightOf="@+id/textView3_wuliaopeisong"
                android:text=""
                android:textColor="@color/black"
                android:textSize="15sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativeLayout4_peisongxiangqing"
            android:layout_width="300dp"
            android:layout_height="25dp"
            android:layout_centerHorizontal="true">
            <TextView
                android:id="@+id/textView4_wuliaopeisong"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="单位："
                android:textColor="@color/black"
                android:textSize="15sp" />
            <View
                android:id="@+id/view1"
                android:layout_width="1dip"
                android:layout_height="fill_parent"
                android:layout_centerVertical="true"
                android:layout_gravity="center_horizontal"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="2dp"
                android:layout_toEndOf="@+id/textView4_wuliaopeisong"/>
            <TextView
                android:id="@+id/danwei"
                android:layout_toEndOf="@+id/view1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/textView4_wuliaopeisong"
                android:text=""
                android:textColor="@color/black"
                android:textSize="15sp" />
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/cancel"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.0"
            android:gravity="center"
            android:text="确定"
            android:textSize="20sp"
            android:background="@color/blue_touch_user"
            android:textColor="@color/black"/>


        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/black"/>


        <TextView
            android:id="@+id/submit"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:layout_weight="1.0"
            android:background="#e1dada"
            android:text="取消"
            android:textSize="20sp"
            android:textColor="@color/black"/>


    </LinearLayout>


</LinearLayout>