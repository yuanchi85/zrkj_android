                        android:layout_width="60dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:lines="1"
                        android:padding="1dp"
                        android:text="扫描条码:"
                        android:textSize="11sp"
                        android:textStyle="bold" />

