                            android:textIsSelectable="false"
                            android:textSize="12sp" />
                    </RelativeLayout>
                </RelativeLayout>
            </LinearLayout>

            <RelativeLayout
                android:id="@+id/relativeLayout15"
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:layout_below="@+id/linearlayout1"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="10dp"
                android:background="@color/background_user"
                android:focusable="true"
                android:focusableInTouchMode="true"
                tools:ignore="UnknownIdInLayout">

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/shape_wihte_frame"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView9_pinguan_yuanliao2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:gravity="center"
                        android:lines="1"
                        android:padding="1dp"
                        android:text="新仓库"
                        android:textSize="@dimen/textSize"
                        android:textStyle="bold" />

                    <View
                        android:id="@+id/view9_pinguan_yuanliao2"
                        android:layout_width="1dip"
                        android:layout_height="fill_parent"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        android:layout_toRightOf="@+id/textView9_pinguan_yuanliao2"
                        android:background="#EEEFFF" />

                    <Spinner
                        android:id="@+id/spinner1"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:entries="@array/Data4"
                        android:paddingLeft="60dp"
                        android:spinnerMode="dropdown" />
                </LinearLayout>
            </RelativeLayout>

            <!--新托盘条码-->
            <RelativeLayout
                android:id="@+id/relativeLayout14"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/heightSize"
                android:layout_below="@+id/relativeLayout15"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="10dp"
                android:background="@color/background_user">

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/shape_wihte_frame">

                    <TextView
                        android:id="@+id/textView14"
                        android:layout_width="60dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:lines="1"
                        android:padding="1dp"
                        android:text="新托盘条码:"
                        android:textSize="@dimen/textSize"
                        android:textStyle="bold" />

                    <View
                        android:id="@+id/view14"
                        android:layout_width="1dip"
                        android:layout_height="fill_parent"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        android:layout_toRightOf="@+id/textView14"
                        android:background="#EEEFFF" />

                    <EditText
                        android:id="@+id/xintuopantiaoma"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_alignParentTop="true"
                        android:layout_toEndOf="@+id/view14"
                        android:layout_toRightOf="@+id/view14"
                        android:background="@drawable/transparent"
                        android:ems="19"
                        android:hint="请扫描托盘条码..."
                        android:imeOptions="actionDone"
                        android:padding="1dp"
                        android:singleLine="true"
                        android:editable="false"
                        android:textSize="@dimen/textSize" />

                </RelativeLayout>
            </RelativeLayout>
            <!--备注-->
            <RelativeLayout
                android:id="@+id/relativeLayout13"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/heightSize"
                android:layout_below="@+id/relativeLayout14"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="10dp"
                android:background="@color/background_user">

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/shape_wihte_frame">

                    <TextView
                        android:id="@+id/textView13"
                        android:layout_width="60dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:lines="1"
                        android:padding="1dp"
                        android:text="备注:"
                        android:textSize="@dimen/textSize"
                        android:textStyle="bold" />

                    <View
                        android:id="@+id/view13"
                        android:layout_width="1dip"
                        android:layout_height="fill_parent"
                        android:layout_centerVertical="true"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginLeft="2dp"
                        android:layout_marginRight="2dp"
                        android:layout_toRightOf="@+id/textView13"
                        android:background="#EEEFFF" />

                    <EditText
                        android:id="@+id/beizhu"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_alignParentTop="true"
                        android:layout_toEndOf="@+id/view13"
                        android:layout_toRightOf="@+id/view13"
                        android:background="@drawable/transparent"
                        android:ems="19"
                        android:hint="请输入备注..."
                        android:imeOptions="actionDone"
                        android:padding="1dp"
                        android:singleLine="true"
                        android:textSize="@dimen/textSize" />

                </RelativeLayout>
            </RelativeLayout>

            <Button
                android:id="@+id/login"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_below="@+id/relativeLayout13"
                android:layout_marginTop="40dp"
                android:background="@drawable/login_button_selector"
                android:padding="1dp"
                android:text="确  定"
                android:textColor="@color/white"
                android:textSize="21sp" />
        </RelativeLayout>
    </ScrollView>


</RelativeLayout>

