<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_user"
    android:scrollbars="none">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="60dp">

        <com.finddreams.baselib.utils.FlowLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <!--分托-->
            <RelativeLayout
                android:id="@+id/relativeLayout_pymian1"
                android:layout_width="101dp"
                android:layout_height="120dp"
                android:clickable="true"
                android:visibility="gone"
                 >

                <ImageView
                    android:id="@+id/imageView_pymian1"
                    android:layout_width="72dp"
                    android:layout_height="72dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/fentuo" />

                <TextView
                    android:id="@+id/textView_pymian1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imageView_pymian1"
                    android:layout_centerHorizontal="true"
                    android:gravity="center"
                    android:text="分  托"
                    android:textSize="16sp" />
            </RelativeLayout>
            <!--入库复核-->
            <RelativeLayout
                android:id="@+id/relativeLayout_pymian2"
                android:layout_width="101dp"
                android:layout_height="120dp"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:clickable="true"
                android:visibility="gone"
                 >

                <ImageView
                    android:id="@+id/imageView_pymian2"
                    android:layout_width="72dp"
                    android:layout_height="72dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/caigoufuhe" />

                <TextView
                    android:id="@+id/textView_pymian2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imageView_pymian2"
                    android:layout_centerHorizontal="true"
                    android:gravity="center"
                    android:text="入库复核"
                    android:textSize="16sp" />
            </RelativeLayout>
            <!--入库质检-->
            <RelativeLayout
                android:id="@+id/relativeLayout_pymian3"
                android:layout_width="101dp"
                android:layout_height="120dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentTop="true"
                android:clickable="true"
                android:visibility="gone"
                 >

                <ImageView
                    android:id="@+id/imageView_pymian3"
                    android:layout_width="72dp"
                    android:layout_height="72dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/fentuo" />

                <TextView
                    android:id="@+id/textView_pymian3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imageView_pymian3"
                    android:layout_centerHorizontal="true"
                    android:gravity="center"
                    android:text="入库质检"
                    android:textSize="16sp" />
            </RelativeLayout>
            <!--破袋调整-->
            <RelativeLayout
                android:id="@+id/relativeLayout_pymian4"
                android:layout_width="101dp"
                android:layout_height="120dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:clickable="true"
                android:visibility="gone"
                 >

                <ImageView
                    android:id="@+id/imageView_pymian4"
                    android:layout_width="72dp"
                    android:layout_height="72dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/fentuo" />

                <TextView
                    android:id="@+id/textView_pymian4"
