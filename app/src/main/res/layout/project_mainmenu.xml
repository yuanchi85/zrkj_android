                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imageView_pymian11"
                    android:layout_centerHorizontal="true"
                    android:gravity="center"
                    android:text="批量销货"
                    android:textSize="16sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relativeLayout_pymian10"
                android:layout_width="101dp"
                android:layout_height="120dp"
                android:layout_alignParentEnd="true"
                android:clickable="true"
                >

                <ImageView
                    android:id="@+id/imageView_pymian10"
                    android:layout_width="72dp"
                    android:layout_height="72dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/fentuo" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imageView_pymian10"
                    android:layout_centerHorizontal="true"
                    android:gravity="center"
                    android:text="库存查询"
                    android:textSize="16sp" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relativeLayout_pymian12"
                android:layout_width="101dp"
                android:layout_height="120dp"
                android:layout_alignParentEnd="true"
                android:clickable="true"
                android:visibility="gone"
                >

                <ImageView
                    android:id="@+id/imageView_pymian12"
                    android:layout_width="72dp"
                    android:layout_height="72dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
