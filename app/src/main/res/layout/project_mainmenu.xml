                    android:layout_width="72dp"
                    android:layout_height="72dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/fentuo" />

                <TextView
                    android:id="@+id/textView_pymian6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imageView_pymian6"
                    android:layout_centerHorizontal="true"
                    android:gravity="center"
                    android:text="销货"
                    android:textSize="16sp" />
            </RelativeLayout>
            <!--销货-->
            <RelativeLayout
                android:id="@+id/relativeLayout_pymian7"
                android:layout_width="101dp"
                android:layout_height="120dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:clickable="true"
                android:visibility="gone"
                 >

                <ImageView
                    android:id="@+id/imageView_pymian7"
                    android:layout_width="72dp"
                    android:layout_height="72dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:src="@drawable/fentuo" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/imageView_pymian7"
                    android:layout_centerHorizontal="true"
                    android:gravity="center"
                    android:text="退货"
                    android:textSize="16sp" />
            </RelativeLayout>

            <RelativeLayout
